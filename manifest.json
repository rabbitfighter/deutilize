{
	"manifest_version": 2,
	"name": "deutilize",
	"version": "1",
	"description": "change 'utilize' to 'use'.",
	"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
	"browser_action": {
		"default_icon": "icon128.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [{
      "matches": ["*://*/*"],
      "js": ["deutilize.js"]
    }]
	
}
